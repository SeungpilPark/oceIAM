<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.opencloudengine.garuda.web.contactus.ContactUsRepository">

    <insert id="insert" parameterType="org.opencloudengine.garuda.web.contactus.ContactUs">
        INSERT INTO CONTACT_US (name, email, subject, telephone, message)
        VALUES (
          #{name}, #{email}, #{subject}, #{telephone}, #{message}
        )
    </insert>
</mapper>